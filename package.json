{
  "name": "php-inline-sql",
  "displayName": "Inline SQL for PHP",
  "description": "Adds SQL syntax highlighting for PHP strings with /*sql*/ prefix",
  "version": "0.0.1",
  "publisher": "ericgomez",
  "icon": "images/icon.jpg",
  "license": "MIT",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "grammars": [
      {
        "path": "./syntaxes/php-sql.tmLanguage.json",
        "scopeName": "source.php.sql",
        "injectTo": [
          "text.html.php"
        ],
        "embeddedLanguages": {
          "source.sql.embedded.php": "sql"
        }
      }
    ]
  },
  "keywords": [
    "php",
    "sql",
    "highlight"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ericgomez/vscode-php-inline-sql.git"
  },
  "bugs": {
    "url": "https://github.com/ericgomez/vscode-php-inline-sql/issues",
    "email": "eric.gomez.sanchez@gmail.com"
  }
}
